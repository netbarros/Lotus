{
  "dashboard": {
    "id": null,
    "uid": "petala-fashion",
    "title": "PÃ©tala Fashion - E-commerce Dashboard",
    "tags": ["petala", "fashion", "ecommerce", "magicsaas"],
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Total Revenue (MRR)",
        "type": "stat",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(petala_fashion_revenue_total{tenant_id=\"$tenant\"})",
            "legendFormat": "Revenue"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "currencyUSD",
          "reduceOptions": { "calcs": ["lastNotNull"] }
        }
      },
      {
        "id": 2,
        "title": "Active Orders",
        "type": "stat",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(petala_fashion_orders_total{tenant_id=\"$tenant\",status=\"active\"})",
            "legendFormat": "Orders"
          }
        ],
        "options": { "graphMode": "area", "colorMode": "value" }
      },
      {
        "id": 3,
        "title": "Total Products",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(petala_fashion_products_total{tenant_id=\"$tenant\",status=\"active\"})",
            "legendFormat": "Products"
          }
        ]
      },
      {
        "id": 4,
        "title": "Conversion Rate",
        "type": "gauge",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "petala_fashion_conversion_rate{tenant_id=\"$tenant\"}",
            "legendFormat": "Rate"
          }
        ],
        "options": {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      },
      {
        "id": 5,
        "title": "Revenue Over Time",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(petala_fashion_revenue_total{tenant_id=\"$tenant\"}[5m])",
            "legendFormat": "{{currency}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Orders by Status",
        "type": "piechart",
        "gridPos": { "x": 12, "y": 4, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum by(status) (petala_fashion_orders_total{tenant_id=\"$tenant\"})",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 7,
        "title": "Top Products",
        "type": "table",
        "gridPos": { "x": 18, "y": 4, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "topk(10, petala_fashion_product_orders{tenant_id=\"$tenant\"})",
            "format": "table"
          }
        ]
      },
      {
        "id": 8,
        "title": "Low Inventory Alerts",
        "type": "table",
        "gridPos": { "x": 0, "y": 12, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "petala_fashion_inventory_low{tenant_id=\"$tenant\"}",
            "format": "table"
          }
        ]
      },
      {
        "id": 9,
        "title": "Cart Abandonment Rate",
        "type": "gauge",
        "gridPos": { "x": 8, "y": 12, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "petala_fashion_cart_abandonment_rate{tenant_id=\"$tenant\"}",
            "legendFormat": "Rate"
          }
        ],
        "options": { "min": 0, "max": 100, "unit": "percent" }
      },
      {
        "id": 10,
        "title": "Average Order Value",
        "type": "stat",
        "gridPos": { "x": 16, "y": 12, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "petala_fashion_avg_order_value{tenant_id=\"$tenant\"}",
            "legendFormat": "AOV"
          }
        ],
        "options": { "unit": "currencyUSD" }
      },
      {
        "id": 11,
        "title": "AR Try-On Sessions",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 18, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(petala_fashion_ar_sessions_total{tenant_id=\"$tenant\"}[5m])",
            "legendFormat": "Sessions/min"
          }
        ]
      },
      {
        "id": 12,
        "title": "Reviews by Rating",
        "type": "bargauge",
        "gridPos": { "x": 12, "y": 18, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "sum by(rating) (petala_fashion_reviews_total{tenant_id=\"$tenant\"})",
            "legendFormat": "{{rating}} stars"
          }
        ]
      },
      {
        "id": 13,
        "title": "API Performance (p95)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(petala_fashion_request_duration_seconds_bucket{tenant_id=\"$tenant\"}[5m]))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "options": { "unit": "s" }
      },
      {
        "id": 14,
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(petala_fashion_api_requests_total{tenant_id=\"$tenant\",status=~\"5..\"}[5m])",
            "legendFormat": "{{endpoint}}"
          }
        ]
      },
      {
        "id": 15,
        "title": "Active Users by Plan",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 30, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "sum by(plan) (petala_fashion_active_users{tenant_id=\"$tenant\"})",
            "legendFormat": "{{plan}}"
          }
        ]
      },
      {
        "id": 16,
        "title": "Geographic Distribution",
        "type": "geomap",
        "gridPos": { "x": 12, "y": 30, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "sum by(country) (petala_fashion_orders_total{tenant_id=\"$tenant\"})",
            "legendFormat": "{{country}}"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(petala_fashion_revenue_total, tenant_id)",
          "current": { "text": "All", "value": "$__all" },
          "multi": false
        }
      ]
    }
  }
}
