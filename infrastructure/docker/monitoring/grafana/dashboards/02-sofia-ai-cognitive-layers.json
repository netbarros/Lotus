{
  "dashboard": {
    "title": "Sofia AI v3.0 - Cognitive Mesh OS - 11 Layers",
    "uid": "sofia-cognitive-layers",
    "version": 1,
    "tags": ["sofia-ai", "cognitive-mesh", "system-11"],
    "timezone": "browser",
    "editable": true,
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Layer 11: Meta-Orchestration - Coordination Events",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 6},
        "targets": [
          {
            "expr": "rate(sofia_meta_orchestration_events_total[5m])",
            "legendFormat": "Orchestration Events/sec - {{event_type}}",
            "refId": "A"
          }
        ],
        "description": "Layer 11 coordinates all other layers and manages cross-layer workflows"
      },
      {
        "id": 2,
        "title": "Layer 10: Intelligence Synthesis (Sofia AI Core)",
        "type": "row",
        "gridPos": {"x": 0, "y": 6, "w": 24, "h": 1},
        "collapsed": false
      },
      {
        "id": 3,
        "title": "IntentionEngine - Processing Time (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 7, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(sofia_intention_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(sofia_intention_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(sofia_intention_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [{"format": "s", "label": "Processing Time"}]
      },
      {
        "id": 4,
        "title": "UXValidator - Validation Score Distribution",
        "type": "graph",
        "gridPos": {"x": 8, "y": 7, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(sofia_ux_validation_score_bucket[5m]))",
            "legendFormat": "Median Score",
            "refId": "A"
          },
          {
            "expr": "avg(sofia_ux_validation_score)",
            "legendFormat": "Average Score",
            "refId": "B"
          }
        ],
        "yaxes": [{"format": "short", "label": "UX Score (0-100)", "min": 0, "max": 100}]
      },
      {
        "id": 5,
        "title": "SEOOptimizer - SEO Grade Distribution",
        "type": "pie",
        "gridPos": {"x": 16, "y": 7, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "sum by (grade) (sofia_seo_grade_total)",
            "legendFormat": "Grade {{grade}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "MarketplaceManager - Checkout Conversion Funnel",
        "type": "graph",
        "gridPos": {"x": 0, "y": 13, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(sofia_marketplace_product_views_total[5m])",
            "legendFormat": "Product Views",
            "refId": "A"
          },
          {
            "expr": "rate(sofia_marketplace_cart_adds_total[5m])",
            "legendFormat": "Add to Cart",
            "refId": "B"
          },
          {
            "expr": "rate(sofia_marketplace_checkout_started_total[5m])",
            "legendFormat": "Checkout Started",
            "refId": "C"
          },
          {
            "expr": "rate(sofia_marketplace_checkout_completed_total[5m])",
            "legendFormat": "Checkout Completed",
            "refId": "D"
          }
        ]
      },
      {
        "id": 7,
        "title": "DecisionLogger - Confidence Score Distribution",
        "type": "heatmap",
        "gridPos": {"x": 12, "y": 13, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(sofia_decision_confidence_bucket[5m])",
            "legendFormat": "Confidence {{le}}",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets"
      },
      {
        "id": 8,
        "title": "Layer 09: Adaptive Learning - Model Performance",
        "type": "graph",
        "gridPos": {"x": 0, "y": 19, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sofia_adaptive_learning_accuracy",
            "legendFormat": "Model Accuracy - {{model}}",
            "refId": "A"
          },
          {
            "expr": "rate(sofia_adaptive_learning_training_cycles_total[1h])",
            "legendFormat": "Training Cycles/hour",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Accuracy"},
          {"format": "short", "label": "Cycles/hour"}
        ]
      },
      {
        "id": 9,
        "title": "Layer 08: Pattern Recognition - Patterns Detected",
        "type": "stat",
        "gridPos": {"x": 12, "y": 19, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(sofia_patterns_detected_total)",
            "legendFormat": "Total Patterns",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 10,
        "title": "Layer 07: Event Sourcing - Events Persisted",
        "type": "graph",
        "gridPos": {"x": 18, "y": 19, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "rate(sofia_events_persisted_total[5m])",
            "legendFormat": "Events/sec - {{event_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "title": "Layer 06: Decision Engine - Decisions/sec by Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 25, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(sofia_decisions_made_total[5m])",
            "legendFormat": "{{decision_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 12,
        "title": "Layer 05: Context Management - Active Contexts",
        "type": "stat",
        "gridPos": {"x": 12, "y": 25, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sofia_active_contexts",
            "legendFormat": "Active Contexts",
            "refId": "A"
          }
        ]
      },
      {
        "id": 13,
        "title": "Layer 04: Service Mesh - Inter-service Communication",
        "type": "graph",
        "gridPos": {"x": 18, "y": 25, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "rate(service_mesh_requests_total[5m])",
            "legendFormat": "{{source}} â†’ {{destination}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 14,
        "title": "Layer 03: Data Layer - Query Performance",
        "type": "graph",
        "gridPos": {"x": 0, "y": 31, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pg_stat_statements_mean_time_seconds_bucket[5m]))",
            "legendFormat": "PostgreSQL p95 Query Time",
            "refId": "A"
          },
          {
            "expr": "redis_command_duration_seconds_p95",
            "legendFormat": "Redis p95 Command Time",
            "refId": "B"
          }
        ],
        "yaxes": [{"format": "s", "label": "Query Time"}]
      },
      {
        "id": 15,
        "title": "Layer 02: Integration Layer - API Response Times",
        "type": "graph",
        "gridPos": {"x": 12, "y": 31, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"directus\"}[5m]))",
            "legendFormat": "Directus API p95",
            "refId": "A"
          }
        ],
        "yaxes": [{"format": "s", "label": "Response Time"}]
      },
      {
        "id": 16,
        "title": "Layer 01: Infrastructure - Resource Utilization",
        "type": "graph",
        "gridPos": {"x": 0, "y": 37, "w": 24, "h": 6},
        "targets": [
          {
            "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU % - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory % - {{instance}}",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"})) * 100",
            "legendFormat": "Disk % - {{instance}}",
            "refId": "C"
          }
        ],
        "yaxes": [{"format": "percent", "label": "Utilization %", "min": 0, "max": 100}]
      }
    ]
  }
}
